<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/back"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".SettingFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:id="@+id/frameLayout3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintWidth_percent="0.9">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.1"
                    android:gravity="center"
                    android:text="설정"
                    android:textColor="@color/text"
                    android:textSize="16sp" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.065"
                    android:text="색상 선택"
                    android:textColor="@color/text"
                    android:textSize="16sp"
                    android:gravity="center_vertical"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.18"
                    android:background="@drawable/back2_round8"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/skin0"
                        android:layout_width="0dp"
                        android:layout_weight="1.5"
                        android:layout_height="match_parent"
                        android:layout_margin="16dp"
                        android:background="@drawable/skin1_back_12"
                        android:text="노란색"
                        android:gravity="center"
                        android:textSize="12sp"
                        android:textColor="@color/black"/>

                    <TextView
                        android:id="@+id/skin1"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:layout_marginVertical="28dp"
                        android:layout_marginRight="4dp"
                        android:background="@drawable/skin1_back_12"
                        android:gravity="center"
                        android:textSize="12sp"
                        android:textColor="@color/black"
                        android:clickable="true"
                        android:focusable="true"/>

                    <TextView
                        android:id="@+id/skin2"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:layout_marginVertical="28dp"
                        android:layout_marginHorizontal="4dp"
                        android:background="@drawable/skin2_back_12"
                        android:gravity="center"
                        android:textSize="12sp"
                        android:textColor="@color/black"
                        android:clickable="true"
                        android:focusable="true"/>

                    <TextView
                        android:id="@+id/skin3"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:layout_marginVertical="28dp"
                        android:layout_marginHorizontal="4dp"
                        android:background="@drawable/skin3_black_12"
                        android:gravity="center"
                        android:textSize="12sp"
                        android:textColor="@color/black"
                        android:clickable="true"
                        android:focusable="true"/>

                    <TextView
                        android:id="@+id/skin4"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:layout_marginVertical="28dp"
                        android:layout_marginHorizontal="4dp"
                        android:background="@drawable/skin4_back_12"
                        android:gravity="center"
                        android:textSize="12sp"
                        android:textColor="@color/black"
                        android:clickable="true"
                        android:focusable="true"/>

                    <TextView
                        android:id="@+id/skin5"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:layout_marginVertical="28dp"
                        android:layout_marginLeft="4dp"
                        android:layout_marginRight="16dp"
                        android:background="@drawable/skin5_back_12"
                        android:textSize="12sp"
                        android:textColor="@color/black"
                        android:clickable="true"
                        android:focusable="true"/>
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.065"
                    android:textColor="@color/text"
                    android:text="고객 지원"
                    android:textSize="16sp"
                    android:gravity="center_vertical" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.25"
                    android:orientation="vertical"
                    android:background="@drawable/back2_round8">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:layout_marginHorizontal="25dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/setText11"
                            android:clickable="true"
                            android:focusable="true"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="bottom"
                            android:text="어플 사용법"
                            android:textColor="@color/text"
                            android:textSize="18sp" />

                        <TextView
                            android:id="@+id/setText12"
                            android:clickable="true"
                            android:focusable="true"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:background="@drawable/under"
                            android:gravity="center_vertical"
                            android:textColor="@color/text"
                            android:text="블루투스 연결, 만들기, 설정"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:layout_marginHorizontal="25dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/setText21"
                            android:clickable="true"
                            android:focusable="true"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:text="출력기 사용법"
                            android:textColor="@color/text"
                            android:textSize="18sp" />

                        <TextView
                            android:id="@+id/setText22"
                            android:clickable="true"
                            android:focusable="true"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="top"
                            android:textColor="@color/text"
                            android:text="전원 키고 끄기, 라벨지 넣기"
                            android:textSize="12sp" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.15"
                    android:orientation="vertical"
                    android:layout_marginTop="12dp"
                    android:background="@drawable/back2_round8">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:layout_marginHorizontal="25dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/setText31"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="bottom"
                            android:text="서비스 소개"
                            android:textColor="@color/text"
                            android:textSize="18sp" />

                        <TextView
                            android:id="@+id/setText32"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:textColor="@color/text"
                            android:text="KOREATECH, 문의 및 건의하기"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.2" />

            </LinearLayout>

        </FrameLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>